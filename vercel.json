{
    "version": 2,
    "builds": [{ "src": "app.py", "use": "@vercel/python" }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production"
    },
    "functions": {
      "api/processing": {
        "runtime": "python3.9",
        "maxDuration": 30
      }
    }
}